'use strict';

/**
 * @ngdoc directive
 * @name verpApp.directive:sp
 * @description
 * # sp creates a scatter plot
 */
angular.module('verpApp')
  .directive('sp', ['FileReader', function(FileReader) {
    return {
      template: '<div></div>',
      restrict: 'E',
      replace: true,
      link: function (scope, element, attrs) {
        var sp = scatter();
      }
    };
  }]);
